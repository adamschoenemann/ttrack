{
  "style": "gibiansky",
  "extensions": [],
  "fixities": [],
  "formatting": {
    "op": {
      ",": {
        "force-linebreak": false,
        "spaces": "after",
        "linebreaks": "before"
      },
      ":": {
        "force-linebreak": false,
        "spaces": "none",
        "linebreaks": "before"
      },
      "=": {
        "force-linebreak": false,
        "spaces": "both",
        "linebreaks": "after"
      },
      "default": {
        "force-linebreak": false,
        "spaces": "both",
        "linebreaks": "before"
      },
      ". in expression": {
        "force-linebreak": false,
        "spaces": "both",
        "linebreaks": "before"
      },
      ". in type": {
        "force-linebreak": false,
        "spaces": "after",
        "linebreaks": "after"
      },
      "record": {
        "force-linebreak": false,
        "spaces": "after",
        "linebreaks": "none"
      }
    },
    "group": {
      "(# in pattern": {
        "force-linebreak": false,
        "spaces": "both",
        "linebreaks": "none"
      },
      "default": {
        "force-linebreak": false,
        "spaces": "none",
        "linebreaks": "none"
      },
      "{": {
        "force-linebreak": false,
        "spaces": "both",
        "linebreaks": "after"
      },
      "(# in expression": {
        "force-linebreak": false,
        "spaces": "both",
        "linebreaks": "none"
      },
      "[": {
        "spaces": "none",
        "linebreaks": "after",
        "force-linebreak": false
      }
    },
    "layout": {
      "infix-app": "try-oneline",
      "if": "vertical",
      "import-spec-list": "flex",
      "con-decls": "vertical",
      "declaration": "flex",
      "app": "try-oneline",
      "let": "vertical",
      "record": "try-oneline",
      "type": "try-oneline",
      "export-spec-list": "vertical",
      "list-comp": "try-oneline"
    },
    "penalty": {
      "overfull": 10,
      "indent": 10,
      "overfull-once": 200,
      "max-line-length": 80,
      "linebreak": 100
    },
    "indent": {
      "deriving": 2,
      "if": "align",
      "let-binds": "align",
      "import-spec-list": "align",
      "onside": 2,
      "where": 2,
      "typesig": "align",
      "do": "indent-by 2",
      "app": "indent-by 2",
      "case": "indent-by 2",
      "let-in": "align",
      "where-binds": "indent-by 2",
      "let": "align",
      "export-spec-list": "indent-by 2",
      "multi-if": "indent-by 2",
      "class": "indent-by 2"
    },
    "align": {
      "let-binds": false,
      "where": false,
      "matches": false,
      "limits": [
        10,
        25
      ],
      "case": false,
      "import-module": true,
      "import-spec": false,
      "class": false,
      "record-fields": false
    },
    "options": {
      "sort-pragmas": true,
      "flexible-oneline": false,
      "decl-no-blank-lines": [],
      "split-language-pragmas": false,
      "align-sum-type-decl": true,
      "sort-import-lists": true,
      "preserve-vertical-space": false,
      "sort-imports": true
    }
  },
  "language": "Haskell2010"
}
